  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-install-plugin</artifactId>
        <version>2.5.2</version>
        <executions>
          <execution>
            <id>install-lib-gdata-base</id>
            <phase>initialize</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <file>${project.build.libDirectory}/gdata-base-1.0.jar</file>
              <groupId>gdata-base</groupId>
              <artifactId>gdata-base</artifactId>
              <version>1.0</version>
              <packaging>jar</packaging>
            </configuration>
          </execution>
          <execution>
            <id>install-lib-gdata-client</id>
            <phase>initialize</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <file>${project.build.libDirectory}/gdata-client-1.0.jar</file>
              <groupId>gdata-client</groupId>
              <artifactId>gdata-client</artifactId>
              <version>1.0</version>
              <packaging>jar</packaging>
            </configuration>
          </execution>
          <execution>
            <id>install-lib-gdata-client-meta</id>
            <phase>initialize</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <file>${project.build.libDirectory}/gdata-client-meta-1.0.jar</file>
              <groupId>gdata-client-meta</groupId>
              <artifactId>gdata-client-meta</artifactId>
