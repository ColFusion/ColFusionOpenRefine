            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <file>${project.build.libDirectory}/google-api-services-fusiontables-v1-rev17-1.13.2-beta.jar</file>
              <groupId>google-api-services-fusiontables</groupId>
              <artifactId>google-api-services-fusiontables</artifactId>
              <version>v1-rev17-1.13.2-beta</version>
              <packaging>jar</packaging>
            </configuration>
          </execution>
          <execution>
            <id>install-lib-google-api-client</id>
            <phase>initialize</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <file>${project.build.libDirectory}/google-api-client-1.13.2-beta.jar</file>
              <groupId>google-api-client</groupId>
              <artifactId>google-api-client</artifactId>
              <version>1.13.2-beta</version>
              <packaging>jar</packaging>
            </configuration>
          </execution>
          <execution>
            <id>install-lib-google-http-client</id>
            <phase>initialize</phase>
            <goals>
              <goal>install-file</goal>
            </goals>
            <configuration>
              <file>${project.build.libDirectory}/google-http-client-1.13.1-beta.jar</file>
              <groupId>google-http-client</groupId>
